    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: nist_bin_adv_class",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "./scripts/nist_binary_adv_classifier.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${cwd}"
            }
        }
    ]
}